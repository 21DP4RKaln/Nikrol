{
  "name": "ivapro-pc-configurator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:high-memory": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev",
    "dev:simple": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NEXT_DEV_SIMPLE=true next dev",
    "dev:reduced": "node scripts/dev-reduced-routes.js",
    "build": "prisma generate --no-engine && cross-env NODE_OPTIONS=--max-old-space-size=3072 NEXT_WEBPACK_CONCURRENCY=1 next build",
    "build:high-memory": "prisma generate --no-engine && cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:low-memory": "prisma generate --no-engine && cross-env NEXT_WEBPACK_CONCURRENCY=1 NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:vercel": "prisma generate --no-engine && cross-env NODE_OPTIONS=--max-old-space-size=3072 NEXT_WEBPACK_CONCURRENCY=1 next build",
    "vercel-build": "npx prisma generate --no-engine && cross-env NODE_OPTIONS=--max-old-space-size=3072 NEXT_WEBPACK_CONCURRENCY=1 next build",
    "build:dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 NODE_ENV=development next build",
    "build:partial": "node scripts/partial-build.js",
    "cleanup": "node scripts/cleanup-before-build.js",
    "build:clean": "npm run cleanup && npm run build:low-memory",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:profile-images": "node test-profile-images.js",
    "setup:vercel-images": "powershell -ExecutionPolicy Bypass -File setup-vercel-images.ps1",
    "debug:hosting-images": "powershell -ExecutionPolicy Bypass -File debug-hosting-images.ps1",
    "fix:hosting-images": "npm run debug:hosting-images && npm run setup:vercel-images",
    "postinstall": "node scripts/prisma-generate.js",
    "prisma:generate": "prisma generate",
    "prisma:generate:prod": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "tsx lib/seeder.ts",
    "migrate:peripherals": "tsx migrate-peripherals.ts",
    "debug:stripe": "node scripts/debug-stripe.js",
    "cleanup:orders": "node scripts/cleanup-orders.js",
    "test:webhook": "node scripts/test-webhook.js",
    "fix:pending": "node scripts/fix-pending-orders.js",
    "deploy:setup": "node scripts/deploy-setup.js",
    "validate:webhook": "node scripts/validate-webhook.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
